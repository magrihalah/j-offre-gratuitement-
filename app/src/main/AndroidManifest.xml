<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.offrirgratuitement">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OffrirGratuitement">
        <activity android:name=".ForgotPasswordActivity"></activity>
        <activity android:name=".PersonalActivity" />
        <activity android:name=".ProductActivity" />
        <activity android:name=".FeedActivity" />
        <activity android:name=".DetailsProductActivity" />
        <activity android:name=".TestActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".JoinActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>